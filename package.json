{
  "name": "eslint-plugin-fsd",
  "version": "0.0.1",
  "description": "Eslint plugin for FSD best practices",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "timon-and-pumbaa",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib && rimraf tests",
    "test": "npm run compile && mocha tests --recursive",
    "lint": "eslint ./src/{lib,tests}/**/* --ext .ts",
    "local-deploy": "npm run compile-prod && npm link",
    "check-types": "tsc",
    "compile-rules": "babel src/lib --out-dir lib --extensions .ts,.js",
    "compile-tests": "babel src/tests --out-dir tests --extensions .ts,.js",
    "compile": "npm run clean && npm run compile-rules && npm run compile-tests",
    "compile-prod": "npm run check-types && npm run lint && npm run test"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^2.23.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@types/eslint": "^6.1.8",
    "@types/estree": "0.0.42",
    "@types/node": "^13.9.1",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "minimist": ">=1.2.2",
    "mocha": "^7.1.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "eslint": "^5.0.0 || ^6.0.0",
    "typescript": ">=3.4.1"
  },
  "engines": {
    "node": ">=10.19.0"
  },
  "license": "ISC"
}
